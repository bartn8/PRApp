<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/statistichemembro_sub_list_pr_item_label_tipoPrevendita"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:layout_gravity="center"
            android:textColor="@color/primaryTextColor"
            tools:text="@string/statistichemembro_sub_list_pr_item_label_tipoPrevendita" />

        <TextView
            android:id="@+id/statistichemembro_sub_list_pr_item_tipoPrevendita"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:layout_gravity="center"
            android:textColor="@color/primaryTextColor"
            tools:text="@string/statistichemembro_sub_list_pr_item_tipoPrevendita" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/statistichemembro_sub_list_pr_item_label_prevenditeVendute"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:layout_gravity="center"
            android:textColor="@color/primaryTextColor"
            tools:text="@string/statistichemembro_sub_list_pr_item_label_prevenditeVendute" />

        <TextView
            android:id="@+id/statistichemembro_sub_list_pr_item_prevenditeVendute"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:layout_gravity="center"
            android:textColor="@color/primaryTextColor"
            tools:text="@string/statistichemembro_sub_list_pr_item_prevenditeVendute" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/statistichemembro_sub_list_pr_item_label_ricavo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:layout_gravity="center"
            android:textColor="@color/primaryTextColor"
            tools:text="@string/statistichemembro_sub_list_pr_item_label_ricavo" />

        <TextView
            android:id="@+id/statistichemembro_sub_list_pr_item_ricavo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:layout_gravity="center"
            android:textColor="@color/primaryTextColor"
            tools:text="@string/statistichemembro_sub_list_pr_item_ricavo" />

    </LinearLayout>

</LinearLayout>